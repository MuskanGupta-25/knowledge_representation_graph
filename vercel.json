{
    "version": 2,
    "builds": [
      {
        "src": "Dockerfile",
        "use": "@vercel/docker"
      },
      {
        "src": "frontend",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ],
    "rewrites": [
      { "source": "/backend/(.*)", "destination": "/api/$1" }
    ]
  }